{
    "name": "beemafrica/php-client",
    "description": "Beem PHP Client",
    "keywords": [
        "wrapper",
        "beem",
        "africa",
        "bulksms"
    ],
    "homepage": "https://github.com/beemafrica/php-client",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Alpha Olomi",
            "email": "alphaolomi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.0|^7.0",
      "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Beem\\SDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Beem\\SDK\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
